  "devDependencies": {
    "@aragon/osx-commons-configs": "^0.2.0",
    "@eslint/eslintrc": "^3.3.1",
    "@types/bun": "latest",
    "@types/dompurify": "^3.0.5",
    "@types/libsodium-wrappers": "^0.7.13",
    "@types/node": "^24.0.0",
    "@types/react": "^19.0.8",
